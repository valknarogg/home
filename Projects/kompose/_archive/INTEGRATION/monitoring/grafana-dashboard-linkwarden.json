{
  "dashboard": {
    "title": "Linkwarden Analytics",
    "tags": ["linkwarden", "bookmarks", "utility"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Total Bookmarks",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "linkwarden_bookmarks_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 2,
        "title": "Bookmarks Added (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "increase(linkwarden_bookmarks_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "title": "Active Users",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "linkwarden_active_users",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Service Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"linkwarden\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "UP"},
              {"type": "value", "value": "0", "text": "DOWN"}
            ],
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Bookmark Creation Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(linkwarden_bookmarks_total[5m])",
            "legendFormat": "Bookmarks/min",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Top Collections",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "topk(10, linkwarden_bookmarks_by_collection)",
            "legendFormat": "{{collection}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 7,
        "title": "Database Query Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(pg_stat_database_tup_fetched{datname=\"linkwarden\"}[5m])",
            "legendFormat": "Rows Fetched",
            "refId": "A"
          },
          {
            "expr": "rate(pg_stat_database_tup_inserted{datname=\"linkwarden\"}[5m])",
            "legendFormat": "Rows Inserted",
            "refId": "B"
          }
        ]
      },
      {
        "id": 8,
        "title": "Redis Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "HTTP Response Times",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{service=\"linkwarden\"}[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"linkwarden\"}[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{service=\"linkwarden\"}[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "MQTT Events Published",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(mqtt_messages_sent_total{topic=~\"kompose/linkwarden/.*\"}[5m])",
            "legendFormat": "{{topic}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 11,
        "title": "Tag Cloud (Top 20)",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "topk(20, linkwarden_bookmarks_by_tag)",
            "legendFormat": "{{tag}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 12,
        "title": "User Activity Timeline",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "rate(linkwarden_user_actions_total[5m])",
            "legendFormat": "{{user}} - {{action}}",
            "refId": "A"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus"
        },
        {
          "name": "user",
          "type": "query",
          "query": "label_values(linkwarden_user_actions_total, user)",
          "datasource": "$datasource",
          "multi": true,
          "includeAll": true
        }
      ]
    }
  }
}
